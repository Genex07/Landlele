def get_bin_info(bin_number):
    url = f"https://lookup.binlist.net/{bin_number}"
    response = requests.get(url)
    if response.status_code != 200:
        return {
            'bank': 'Unknown Bank',
            'country': 'Unknown Country',
            'brand': 'Unknown',
            'type': 'Unknown',
            'level': 'Unknown',
            'flag': 'ğŸ³ï¸'
        }
    
    bin_data = response.json()
    bank = bin_data.get('bank', {}).get('name', 'Unknown Bank')
    country = bin_data.get('country', {}).get('name', 'Unknown Country')
    country_flag = bin_data.get('country', {}).get('emoji', 'ğŸ³ï¸')
    brand = bin_data.get('scheme', 'Unknown')
    card_type = bin_data.get('type', 'Unknown')
    level = bin_data.get('brand', 'Unknown')

    return {
        'bank': bank,
        'country': country,
        'brand': brand,
        'type': card_type,
        'level': level,
        'flag': country_flag
    }

